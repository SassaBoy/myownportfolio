<section class="section bg-custom-gray" id="portfolio">
    <div class="container">
        <h1 class="mb-5"><span class="text-danger">My</span> Portfolio</h1>
        <div class="portfolio">
            <div class="filters">
                <a href="#" class="active">All</a>
            </div>
    
            <div class="row portfolio-container">
                <!-- Check if portfolioItems exist -->
                <% if (portfolioItems && portfolioItems.length > 0) { %>
                    <!-- Loop through portfolioItems and render each item -->
                    <% portfolioItems.forEach(item => { %>
                        <div class="col-md-6 col-lg-3 web new">
                            <div class="portfolio-item">
                                <img src="<%= item.image %>" class="img-fluid" alt="<%= item.title %>">
                                <div class="content-holder">
                                    <a class="img-popup" href="<%= item.imageUrl %>"></a>
                                    <div class="text-holder">
                                        <!-- Modify the title link to point to the image URL -->
                                        <a class="title" href="<%= item.imageUrl %>" target="_blank"><%= item.title %></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% }); %>
                <% } else { %>
                    <!-- Handle case when there are no portfolio items -->
                    <p>No portfolio items available.</p>
                <% } %>
            </div>
        </div>
    </div>
</section>


